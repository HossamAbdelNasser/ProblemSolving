class Solution {
public:

    int memo[50];
    
    int climbStairs(int n) {

        if (memo[n]) return memo[n];

        if (n == 1) return 1;
        if (n == 2) return 2;
        if (n == 3) return 3;

        memo[n] = climbStairs(n-1) + climbStairs(n-2);

         return memo[n];
        
    }
};
