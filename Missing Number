class Solution {
public:
    int missingNumber(vector<int>& nums) {
        
        set <int> s;

        for (auto n : nums) {
            s.insert(n);
        }

        for (int i = 0; i < nums.size(); i++) {

            if (!s.count(i)) return i;
        }
        return nums.size();
    }
};
